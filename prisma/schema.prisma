generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model task {
  id        Int      @id @default(autoincrement())
  title      String
  description     String
  status Boolean?
  created_at DateTime @default(now())
  updated_at DateTime @default(now())
  finished_at DateTime @default(now())
}
